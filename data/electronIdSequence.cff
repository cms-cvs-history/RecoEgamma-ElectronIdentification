include "RecoEgamma/ElectronIdentification/data/electronIdCutBasedExt.cfi"

module eidRobustLoose = eidCutBasedExt from "RecoEgamma/ElectronIdentification/data/electronIdCutBasedExt.cfi"

module eidRobustTight = eidCutBasedExt from "RecoEgamma/ElectronIdentification/data/electronIdCutBasedExt.cfi"
replace eidRobustTight.robustEleIDCuts.barrel = {0.015,0.092,0.020,0.0025}
replace eidRobustTight.robustEleIDCuts.endcap = {0.018,0.025,0.020,0.0040}

module eidLoose = eidCutBasedExt from "RecoEgamma/ElectronIdentification/data/electronIdCutBasedExt.cfi"
replace eidLoose.electronQuality = "loose"

module eidTight = eidCutBasedExt from "RecoEgamma/ElectronIdentification/data/electronIdCutBasedExt.cfi"
replace eidTight.electronQuality = "tight"

sequence eIdSequence = { eidRobustLoose & 
			 eidRobustTight & 
			 eidLoose & 
			 eidTight }
